  "dependencies": {
    "angular2": "2.0.0-beta.0",
    "angular2-universal-preview": "0.24.0",
    "express": "4.13.3",
    "xhr2": "0.1.3"
  },
  "devDependencies": {
    "ts-loader": "0.7.2",
    "typescript": "1.7.5",
